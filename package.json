  "type": "module",
  "scripts": {
    "dev": "concurrently \"tsx src/server.ts\" \"vite\"",
    "dev:frontend": "vite",
    "dev:backend": "tsx src/server.ts",
    "build": "vite build && tsc -p tsconfig.server.json --outDir dist",
    "prebuild": "rimraf dist",
    "preview": "vite preview",
    "start": "NODE_ENV=production node dist/server.js",
    "vercel-build": "npm run build",
    "postinstall": "npm run build",
    "test": "cross-env NODE_OPTIONS='--experimental-vm-modules' jest --config=jest.config.js --verbose",
    "test:watch": "npm test -- --watch",
    "test:coverage": "cross-env NODE_OPTIONS='--experimental-vm-modules' jest --coverage",
    "test:ci": "cross-env NODE_ENV=test NODE_OPTIONS='--experimental-vm-modules' jest --ci --runInBand",
    "test:debug": "cross-env NODE_OPTIONS='--inspect-brk --experimental-vm-modules' jest --config=jest.config.js --runInBand --no-cache",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "db:generate": "drizzle-kit generate:sqlite",
    "db:migrate": "drizzle-kit push:sqlite",
    "db:studio": "drizzle-kit studio"
  },
